{
  "apps": [
    {
      "name": "sandbox",
      "script": "app/server.js",
      "error_file": "app/logs/process-error.log",
      "out_file": "app/logs/process-out.log",
      "exec_mode": "fork",
      "instances": 1,
      "max_restarts": 5,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "sandbox"
      }
    }
  ]
}
