{
  "apps": [
    {
      "name": "sandbox.d-pac",
      "script": "app/server.js",
      "error_file": "app/logs/sandbox.process-error.log",
      "out_file": "app/logs/sandbox.process-out.log",
      "exec_mode": "fork",
      "instances": 1,
      "max_restarts": 5,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "sandbox"
      }
    },
    {
      "name": "demo.d-pac",
      "script": "app/server.js",
      "exec_interpreter": "/usr/local/n/versions/0.12.4/bin/node",
      "error_file": "app/logs/demo.process-error.log",
      "out_file": "app/logs/demo.process-out.log",
      "exec_mode": "fork",
      "instances": 1,
      "max_restarts": 5,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "demo"
      }
    }
  ]
}
